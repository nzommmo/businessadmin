{
  "version": 2,
  "builds": [
    { "src": "Frontend/Businessadmin/package.json", "use": "@vercel/react" },
    { "src": "Backend/businessadmin/wsgi.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/businessadmin/wsgi.py" },
    { "src": "/(.*)", "dest": "/Businessadmin/build/$1" }
  ]
}
